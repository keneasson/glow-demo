{
  "compilerOptions": {
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "paths": {
      "app/*": [
        "./packages/app/*"
      ],
      "@my/ui/*": [
        "./packages/ui/*"
      ]
    },
    "moduleSuffixes": [
      ".ios",
      ".android",
      ".native",
      ".web",
      ""
    ]
  },
  "extends": "expo/tsconfig.base",
  "exclude": [
    "**/node_modules",
    "**/dist",
    "**/types",
    "apps/next/out",
    "apps/next/.next",
    "apps/next/.tamagui"
  ]
}
